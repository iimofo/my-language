{
    "$schema": "https://raw.githubusercontent.com/microsoft/vscode/main/extensions/theme-defaults/schemas/theme.schema.json",
    "name": "MyLang",
    "scopeName": "source.mylang",
    "patterns": [
      {
        "match": "\\b(let|print)\\b",
        "name": "keyword.control.mylang"
      },
      {
        "match": "\\b\\d+\\b",
        "name": "constant.numeric.mylang"
      },
      {
        "match": "\\\"[^\\\"]*\\\"",
        "name": "string.quoted.double.mylang"
      },
      {
        "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b",
        "name": "variable.mylang"
      },
      {
        "match": "\\/\\/.*$",
        "name": "comment.line.double-slash.mylang"
      }
    ],
    "repository": {},
    "scopeName": "source.mylang"
  }
  